{
  "name": "picoDeploy",
  "description": "An Ionic project",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "main": "main.js",
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "lint": "ionic-app-scripts lint",
    "cart": "cp -r cart www/",
    "build": "npm run ionic:build",
    "serve": "npm run ionic:serve",
    "ionic:build": "npm run cart && ionic-app-scripts build",
    "ionic:serve": "npm run cart &&  ionic-app-scripts serve",
    "electron:build": "npm run cart &&  npm run ionic:build --prod && electron-builder",
    "electron:serve": "npm run cart &&  npm run ionic:build --prod && ./node_modules/.bin/electron .",
    "electron:pack": "npm run cart &&  npm run ionic:build --prod && electron-builder --dir"
  },
  "build": {
    "appId": "com.example.picoDeploy",
    "mac": {
      "category": "public.app-category.games"
    }
  },
  "dependencies": {
    "@angular/common": "4.1.3",
    "@angular/compiler": "4.1.3",
    "@angular/compiler-cli": "4.1.3",
    "@angular/core": "4.1.3",
    "@angular/forms": "4.1.3",
    "@angular/http": "4.1.3",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "@ionic-native/core": "3.12.1",
    "@ionic-native/splash-screen": "3.12.1",
    "@ionic-native/status-bar": "3.12.1",
    "@ionic/storage": "2.0.1",
    "ionic-angular": "3.6.1",
    "ionicons": "3.0.0",
    "rxjs": "5.4.0",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@ionic/app-scripts": "2.1.4",
    "electron": "^1.7.6",
    "electron-builder": "^19.27.7",
    "typescript": "2.3.4"
  }
}
